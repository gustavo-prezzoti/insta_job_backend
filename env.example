# Diretório de arquivos temporários
TEMP_DIR=./temporary

# JWT Configuration
JWT_SECRET=ultra_secret_key_change_in_production
JWT_EXPIRATION_DAYS=7

# Supabase Configuration
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_KEY=your-supabase-anon-key

# AWS SES Configuration
AWS_REGION=us-east-1
AWS_SES_SENDER=no-reply@example.com
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key

# SOAX Proxy Configuration (opcional, para instagrapi legacy)
SOAX_USERNAME=your_soax_username
SOAX_PASSWORD=your_soax_password
SOAX_API_KEY=your_soax_api_key

# CORS Settings
CORS_ALLOW_ORIGINS=*

# Webhook Configuration
WEBHOOK_SECRET_TOKEN=webhook_secret_change_in_production

# Domain for URLs
DOMAIN=http://localhost:8080

# Instagram Graph API Configuration
# Obtenha estas credenciais no Meta for Developers Portal (https://developers.facebook.com)
INSTAGRAM_CLIENT_ID=your_app_id
INSTAGRAM_CLIENT_SECRET=your_app_secret
INSTAGRAM_REDIRECT_URI=http://localhost:8080/instagram/oauth/callback
INSTAGRAM_GRAPH_API_VERSION=v19.0
FACEBOOK_GRAPH_API_BASE_URL=https://graph.facebook.com 